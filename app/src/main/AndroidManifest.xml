<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.forest.numbertowordgame">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".CategoryActivity" />
        <activity android:name=".PlayActivity" />
        <activity android:name=".OptionsActivity" />

        <meta-data
            android:name="com.google.android.gms.ads.APPLICATION_ID"
            android:value="@string/google_ad_mod_id" />

        <activity android:name="com.forest.numbertowordgame.RegisterActivity" />
        <activity android:name=".RegisterActivity" />
        <activity android:name=".GrammarActivity" />
        <activity android:name=".SpellingActivity" />
        <activity android:name=".PronounceActivity"></activity>
    </application>

</manifest>